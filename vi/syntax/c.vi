keyword	cStatement goto break return continue asm
keyword	cLabel case default
keyword	cConditional if else switch
keyword	cRepeat while for do
keyword cComments \/\*[\s\S]*\*\/
keyword cFunction \s?main(+ \s?printf(+ \s?exit(+
keyword cBrackets ( )
keyword	cHead \s?#include\s+
keyword	cType \s?int\s+ \s?char\s+ \s?int\n

hi cStatement GREEN
hi cLabel BLUE
hi cConditional LIGHT_RED
hi cRepeat LIGHT_RED
hi cComments GREY
hi cFunction YELLOW
hi cBrackets WHITE
hi cHead LIGHT_RED
hi cType CYAN